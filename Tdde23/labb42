def interpret(list_items, dictionary):
    # list_items = li[0]
    # dictionary = li[1]
    if not list_items:
        print('if')
        return 0
    elif isinstance(list_items[0], list):
        print('elif')
        sum_of_element = interpret(list_items[0], dictionary)
        sum_of_rest_list = interpret(list_items[1:0], dictionary)
        return sum_of_element + sum_of_rest_list
    else:
        print('else')
        op = 'addition'
        element = list_items[0]
        if dictionary.get(element) == 'true':
            element = 0
        elif dictionary.get(element) == 'false':
            element = 1
        elif element == 'NOT':
            element = 1
        elif element == 'AND':
            element = 0
        elif element == 'true':
            element = 0
        elif element == 'false':
            element = 1
        elif element == 'OR':
            return 'OR'
            
        sum_of_rest_list = interpret(list_items[1:], dictionary)
        if sum_of_rest_list == 'OR':
            op = 'mult'
            sum_of_rest_list = interpret(list_items[2:], dictionary)

        if (op == 'addition'):
            return element + sum_of_rest_list
        else:
            op == 'mult'
            return element * sum_of_rest_list
